{
  "name": "koa-html-minifier",
  "description": "minify HTML responses like some crazy guy",
  "version": "1.0.1",
  "author": {
    "name": "Jonathan Ong",
    "email": "me@jongleberry.com",
    "url": "http://jongleberry.com",
    "twitter": "https://twitter.com/jongleberry"
  },
  "license": "MIT",
  "repository": "koajs/html-minifier",
  "dependencies": {
    "html-minifier-terser": "^5.1.1"
  },
  "devDependencies": {
    "eslint-config-xo-lass": "^1.0.3",
    "koa": "^2.13.0",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "supertest": "^4.0.2",
    "xo": "^0.32.1"
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass"
    ],
    "ignores": [
      "test"
    ],
    "rules": {
      "func-names": "off"
    }
  },
  "scripts": {
    "lint": "xo",
    "test": "mocha --reporter spec",
    "test-cov": "node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha",
    "test-travis": "node  ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly --"
  },
  "files": [
    "index.js"
  ]
}
